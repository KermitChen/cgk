<?xml version="1.0" encoding="UTF-8"?>
<root>
    <!-- 用于标识系统， 1.深圳交警   2.乌海   3.宝安/龙华-->
    <sysFlag>1</sysFlag>
	<!--
		日志存放文件目录 ,注意 Linux：/home/esLog/dyst/ windows：D:/esLog/dyst
	-->
	<logFolder>D:\\picLog</logFolder>
	<!-- 定时加载监测点  -->
	<getJcdTime>1800000</getJcdTime>


	<!-- ++++++++++++++++++mysql数据库配置信息++++++++++++++++++++++ -->
	<!--生产库mysql数据库用户名-->
	<user>root</user>
	<!--生产库mysql数据库密码
		深圳：jcbkzhzx110
	-->
	<pwd>jcbkzhzx110</pwd>
	<!--
		生产库mysql数据库URL,IP,端口号，实例名 
		深圳：10.42.127.81:3306
	-->
	<url>jdbc:mysql://10.42.127.81:3306/dc?useUnicode=true&amp;characterEncoding=utf-8</url>
	<!--生产库mysql数据库驱动-->
	<driver>com.mysql.jdbc.Driver</driver>
	<!--生产库mysql数据库连接池最大允许连接数-->
	<dbMaxCon>30</dbMaxCon>
	<!--生产库mysql数据库默认初始化连接数-->
	<dbInit>4</dbInit>
	<!-- 连接池连接超时时间，默认为10000毫秒 -->
	<dbtimeOut>1000</dbtimeOut>


	<!-- 图片地址查询时，一次性查询个数，即线程数，最佳配置为机器CPU数 -->
	<threadNum>10</threadNum>
	
	
	<!--
		+++++++++++++++++++++++++++接口查询中涉及到的错误码配置 +++++++++++++++++++++++++++
	-->
